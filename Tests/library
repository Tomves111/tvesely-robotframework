from robot.api import logger
from robot.errors import ExecutionFailed

class library(object)

ROBOT_LIBRARY_VERSION = 1.0

def just_call(self)
    logger.info("Hello World")
    logger.console("Hello world")

    def log_input(input)
        logger.info(input)
        logger.console(input)

def compare_something(self, input):
   self.input = int(input)
    if input < 5:
        logger.info("TestPass because value %s is lower than 5" % input, also_console=True)
    else input = 5:
        logger.warn("%s is equal to 5 and will be reach the threshold soon" % input)
    else:
    logger.error("The value %s reached treshold 5!!!")
    raise ExecutionFailed("Value five was reached!!!")

    def add_values(self, a, b)
        rersult = int(a) + b
        return resault
